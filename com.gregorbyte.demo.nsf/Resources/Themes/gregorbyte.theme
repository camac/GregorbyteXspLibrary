	<!--		<property>-->
	<!--			<name>equalStyle</name>-->
	<!--			<value>color: blue;</value>-->
	<!--		</property>-->
	<!--	</control>-->

	<!--
		Use this pattern to define property name/value pairs for controls
	-->
	<!--
		<control> <name>[Control Name]</name> <property> <name>[property
		Name]</name> <value>[property Value]</value> </property> </control>
	-->

	<control>
		<name>PanelGrid.Black</name>
		<property>
			<name>columnClasses</name>
			<value>columnBlack</value>
		</property>

		<property>
			<name>cellpadding</name>
			<value>5</value>
		</property>

		<property>
			<name>cellspacing</name>
			<value>0</value>
		</property>

		<property>
			<name>frame</name>
			<value>box</value>
		</property>


	</control>
	
	<control>
		<name>Text.Diff.Funky</name>
		<property>
			<name>style</name>
			<value>background-color: black;</value>
		</property>
		
		<property>
			<name>insertStyle</name>
			<value>color: aqua;</value>
		</property>
		<property>
			<name>equalStyle</name>
			<value>color: lime;</value>
		</property>
		<property>
			<name>deleteStyle</name>
			<value>color: pink;</value>
		</property>
	
	</control>

</theme>
